ui-tabs {
  background-color: #f5f5f5;
  padding-bottom: 1px;
}
.content {
  height: 60px;
  line-height: 60px;
  margin: 0 10px;
}
.sorting-img {
  width: 100%;
  height: 50%;
}
.garbage {
  display: flex;
  width: 100%;
  height: 100rpx;
  border-bottom: solid 1px #d3d3d3;
  align-items: center;
}
.searchbar {
  width: 100%;
  height: 90rpx;
  background-color: #ffffff;
}
.searchinput {
  float: left;
  width: 70%;
  height: 90%;
  background-color: #f5f5f5;
  border-radius: 20rpx;
  margin-left: 3%;
  padding-left: 2%;
}
.searchbutton {
  float: right;
  width: 20%;
  margin: 0;
  padding: 0;
  margin-right: 3%;
  height: 90%;
}
.popup5 {
  width: 95%;
  background-color: #fff;
  height: 100%;
  margin: 0 auto;
  border-radius: 5px;
  overflow: hidden;
}
.popup5 .sortingimg {
  height: 60%;
  margin: auto;
}
.popup5top {
  width: 100%;
  height: 33%;
}
.popup5top .name {
  text-align: left;
  line-height: 200%;
  margin-left: 5%;
  font-size: 150%;
  color: white;
}
.popup5top .sortingname {
  text-align: left;
  line-height: 200%;
  margin-left: 5%;
  font-size: 120%;
  color: white;
}
.popup5top .esortingname {
  text-align: left;
  line-height: 230%;
  margin-left: 5%;
  font-weight: 80%;
  color: white;
}
.popup5top .space {
  height: 5%;
}
.statement {
  width: 90%;
  height: 10%;
  border-top: solid 1rpx #d4d4d4;
  margin-left: 5%;
  text-align: center;
  color: #d4d4d4;
}
